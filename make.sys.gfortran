.SUFFIXES :
.SUFFIXES : .o .f90

.f90.o:
	$(F90) $(F90FLAGS) -cpp -c $(INCLUDES) $<


F90=gfortran
F90FLAGS=-ffree-form -ffree-line-length-none

INCLUDES=-I../modules -I.

LAPACKLIBS=/usr/local/lib/liblapack.a /usr/local/lib/blas.a
LAPACK95LIBS=/usr/local/lib/lapack95.a


AR             = ar
ARFLAGS        = ruv

RANLIB         = ranlib
